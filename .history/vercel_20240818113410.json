{
  "version": 2,
  "routes": [
    { "src": "/homePage", "dest": "/" },
    { "src": "/authenticate", "dest": "/" },
    { "src": "/redirectLinks", "dest": "/" },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "api/redirect.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/index.jsx",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}
