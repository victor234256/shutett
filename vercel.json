{
  "builds": [
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/api/redirect",
      "dest": "/api/redirect.js"
    }
  ]
}
