{
  "builds": [
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/homePage",
      "dest": "/index.html"
    },
    {
      "src": "/authenticate",
      "dest": "/index.html"
    },
    {
      "src": "/:shorturl",
      "dest": "/index.html"
    }
  ]
}
